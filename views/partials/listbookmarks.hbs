{{#each bookmarks}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Click Icons to <br>
      Show &nbsp;/&nbsp; Delete &nbsp;Bookmarks</p>
    <a href="/bookmark/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/bookmarkDashboard/deletebookmark/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}